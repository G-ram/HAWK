OBJS = parser.cmo scanner.cmo prettyprint.cmo semantics.cmo main.cmo

prog : $(OBJS)
	ocamlc -o prog $(OBJS)

.PHONY : test
test : prog testall.sh
	./testall.sh

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -f prog parser.ml parser.mli scanner.ml testall.log \
	*.cmo *.cmi *.out *.cmx *.diff *.exe

# Generated by ocamldep *.ml *.mli
prog.cmo:  scanner.cmo parser.cmi ast.cmi sast.cmi prettyprint.cmo semantics.cmo
prog.cmx: scanner.cmx parser.cmx sast.cmx ast.cmi
parser.cmo: ast.cmi parser.cmi
parser.cmx: ast.cmi parser.cmi
scanner.cmo: parser.cmi
scanner.cmx: parser.cmx
semantics.cmo: sast.cmi
parser.cmi: ast.cmi
